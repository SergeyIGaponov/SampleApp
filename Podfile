# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'Tap2Free' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Tap2Free
  pod 'RxSwift'#,    '~> 4.0'
  pod 'RxCocoa'#,    '~> 4.0'
  pod 'Moya/RxSwift'
  pod 'Moya-ObjectMapper/RxSwift'
  pod 'SDWebImage'
  pod 'SideMenu', '~> 5.0.3'
  pod 'SwiftOverlays'
  pod 'Cosmos'
  pod 'Fabric'
  pod 'Crashlytics'
#  pod 'Reveal-SDK', '20', :configurations => ['Debug']
  pod 'OptimalLabelTextSize'
  pod 'PlainPing'
  pod 'OpenSSL-XM'
  pod 'Google-Mobile-Ads-SDK'
end

target 'Tap2FreeNetworkExtension' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'OpenVPNAdapter', :git => 'https://github.com/ss-abramchuk/OpenVPNAdapter.git', :tag => '0.3.0'
end
